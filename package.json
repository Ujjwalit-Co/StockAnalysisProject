{
  "name": "nextjs_tailwind_shadcn_ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"next dev -p 3000\" --watch src --ext ts,tsx,js,jsx",
    "build": "next build && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "start": "NODE_ENV=production node .next/standalone/server.js 2>&1 | tee server.log",
    "lint": "next lint",
    "db:push": "dotenv -e .env.local prisma db push",
    "db:generate": "dotenv -e .env.local prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:new": "dotenv -e .env.local npx prisma migrate dev --name init_postgresql",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "nodemon": "^3.1.11",
    "p-limit": "^7.2.0",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "prisma": "^6.11.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "xlsx": "^0.18.5",
    "yahoo-finance2": "^3.10.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/papaparse": "^5.5.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4.1.17",
    "typescript": "^5"
  }
